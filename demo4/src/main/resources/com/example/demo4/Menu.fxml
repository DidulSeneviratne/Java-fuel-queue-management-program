<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="357.0" prefWidth="406.0" spacing="20.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo4.MenuController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

   <AnchorPane prefHeight="336.0" prefWidth="366.0">
      <children>
          <Button id="Click" layoutX="230.0" layoutY="69.0" onAction="#onDisplayClick" text="DISPLAY" />
         <TextField id="Queue" fx:id="queueEnter" layoutX="244.0" layoutY="-12.0" prefHeight="25.0" prefWidth="122.0" promptText="1/2/3/4/5/Wait" />
         <Label fx:id="name" layoutX="14.0" layoutY="28.0" text="Search by customer name:- " />
         <TextField id="Name" fx:id="nameEnter" layoutX="171.0" layoutY="24.0" prefHeight="25.0" prefWidth="195.0" promptText="First Name" />
         <TextArea id="Display" fx:id="Display" layoutX="14.0" layoutY="106.0" prefHeight="200.0" prefWidth="352.0" />
         <Label fx:id="queue" layoutX="14.0" layoutY="-8.0" text="Enter queue number you want to dispaly:-" />
         <Button layoutX="306.0" layoutY="69.0" mnemonicParsing="false" onAction="#onSearchClick" text="SEARCH" />
      </children>
   </AnchorPane>
</VBox>
